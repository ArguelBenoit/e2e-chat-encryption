
{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "commonjs": true
  },
  "rules": {
    "comma-dangle": 1,
    "no-console": 0,
    "no-trailing-spaces": 1,
    "quotes": [2, "single"],
    "semi": 1,
    "strict": 0
  }
}
